<h1>Quantity</h1>

<table class="table table-dark table-striped">
  <thead>
    <tr>
      <th>Name</th>
    
     
      <th>Actions</th>
    </tr>
  </thead>
  <tbody>
    <% @quantities.each do |quantity| %>
      <tr>
      <td><%= quantity.name %> </td>
      
    
      <td><a href="/quantities/<%= quantity.id %>/edit">Edit</a></td>
      <td><a href="/quantities/<%= quantity.id %>/destroy">Delete</a></td>
      </tr>
    <% end %> 
  </tbody>
</table>
 
<a href="/quantities/form">Add</a>